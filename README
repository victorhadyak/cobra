// Existing script remains the same up to the fetch call...

// Upload file
fetch(signedUrl, {
    method: 'PUT',
    headers: {
        "Content-Type": file.type
    },
    body: file
})
.then(response => {
    if(response.ok) {
        console.log('File uploaded!');
        alert('File uploaded successfully!');
    } else {
        throw new Error('Upload failed');
    }
})
.catch(error => {
    console.error(error);
    alert('Failed to upload file.');
});

function uploadFile() {
    var file = document.getElementById('fileUpload').files[0];
    
    // Replace with your actual upload URL
    var uploadUrl = 'YOUR_UPLOAD_URL/' + file.name;

    fetch(uploadUrl, {
        method: 'PUT',
        body: file // Send the file directly as the body
    })
    .then(response => {
        if(response.ok) {
            alert('Successfully Uploaded!');
        } else {
            throw new Error('Upload failed');
        }
    })
    .catch(error => {
        console.error(error);
        alert('Failed to upload file.');
    });
}

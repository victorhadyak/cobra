
    cmdOutput, err := cmd.Output()
    if err != nil {
        fmt.Printf("Error listing instances in region %s: %s\n", region, err)
        return
    }

    // Define a structure to parse the output
    var instances [][]string
    if err := json.Unmarshal(cmdOutput, &instances); err != nil {
        fmt.Println("Error parsing JSON:", err)
        return
    }

    // Check if no instances were found
    if len(instances) == 0 {
        fmt.Println("No instances found in this region.")
        return
    }

    // Iterate over the parsed instances and print them
    for i, instance := range instances {
        instanceID := instance[0]
        instanceName := "No Name Tag"
        if len(instance) > 1 && len(instance[1]) > 0 {
            instanceName = instance[1]
        }
        fmt.Printf("%d. %s (%s)\n", i+1, instanceID, strings.Join(instance[1:], ", "))
    }
}

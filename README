import json
import datetime

def convert_timestamp(timestamp_millis):
    # Convert milliseconds to seconds
    timestamp_secs = int(timestamp_millis) / 1000
    # Convert to datetime object
    dt_object = datetime.datetime.fromtimestamp(timestamp_secs)
    return dt_object.strftime("%Y-%m-%d %H:%M:%S")

def process_file(input_file_path, output_file_path):
    with open(input_file_path, 'r') as infile, open(output_file_path, 'w') as outfile:
        for line in infile:
            try:
                log_entry = json.loads(line)
                if 'Time' in log_entry:
                    log_entry['Time'] = convert_timestamp(log_entry['Time'])
                json.dump(log_entry, outfile)
                outfile.write('\n')
            except json.JSONDecodeError:
                print("Error decoding JSON from line:", line)

# Replace with your file paths
input_file_path = 'path_to_your_input_file.json'
output_file_path = 'path_to_your_output_file.json'
process_file(input_file_path, output_file_path)
